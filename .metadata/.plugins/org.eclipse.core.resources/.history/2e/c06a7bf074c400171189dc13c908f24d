package com.assignment_4.superclasses;

public class Human {

	
		
	// *** Attributes ***
	
	private String name = "";
	private int age = 0;
	
		
	// *** Constructor ***
	/**
	 * Constructor med parametrarna name och age
	 * @param name
	 * @param age
	 */
	public Human(String name, int age) {
		//super();
		this.name = name;
		this.age = age;
	}
	
	// *** Getters & Setters ***
	/**
	 * H채mtar namnet
	 * @return name
	 */
	public String getName() {
		return name;
	}

	/**
	 * H채mtar 책lders
	 * @return age
	 */
	public int getAge() {
		return age;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	
}
