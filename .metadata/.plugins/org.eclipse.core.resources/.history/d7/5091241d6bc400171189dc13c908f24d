
public class Human {

	
	// *** Attributes ***
	private String name = "";
	private int age = 0;
	
	// *** Constructor ***
	public Human(String name, int age) {
		//super();
		this.name = name;
		this.age = age;
	}
	// *** Getters & Setters ***
	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	
}
